{{#each this.versions as |version|~}}
  {{#if (lf-or @renderWhenFalse version.value)~}}
    <LiquidChild
      @version={{version}}
      @uniqueChildId={{version.uniqueChildId}}
      @liquidChildDidRender={{this.childDidRender}}
      class={{@class}}
    >
      {{~yield version.value~}}
    </LiquidChild>
  {{~/if}}
{{~/each~}}
<div class="liquid-versions"
  {{this.appendVersion}}
  {{this.appendVersion @value @use @rules @matchContext @versionEquality @renderWhenFalse @class @notify}}
></div>
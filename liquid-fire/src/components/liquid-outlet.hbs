<LfGetOutletState as |outletState|>
  {{~!~}}
  <LiquidBind
    @value={{(lf-lock-model outletState this.outletName)}}
    @containerId={{@containerId}}
    @versionEquality={{this.versionEquality}}
    @matchContext={{(hash outletName=this.outletName helperName="liquid-outlet")}}
    @class={{@class}}
    @use={{@use}}
    @rules={{@rules}}
    @containerless={{@containerless}}
    @containerElement={{@containerElement}}
    @growDuration={{@growDuration}}
    @growPixelsPerSecond={{@growPixelsPerSecond}}
    @growEasing={{@growEasing}}
    @shrinkDelay={{@shrinkDelay}}
    @growDelay={{@growDelay}}
    @enableGrowth={{@enableGrowth}} as |version|
  >
  {{~!~}}
    {{~#-with-dynamic-vars outletState=version~}}
      {{!-- template-lint-disable no-outlet-outside-routes --}}
      {{~outlet~}}
    {{~/-with-dynamic-vars~}}
  {{~!~}}
  </LiquidBind>
  {{~!~}}
</LfGetOutletState>
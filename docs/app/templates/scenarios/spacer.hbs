<button type="button" {{on "click" this.addThing}}>More Things</button>
<button type="button" {{on "click" this.removeThing}}>Less Things</button>

<LiquidSpacer @id="spacer-test">
  <ol>
  {{#each this.things key="number" as |thing|}}
    <li>{{thing.number}}</li>
  {{/each}}
  </ol>
</LiquidSpacer>
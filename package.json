{
  "name": "liquid-fire",
  "description": "Animated transitions for Ember applications.",
  "version": "0.35.0",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:fastboot": "qunit fastboot-tests"
  },
  "dependencies": {
    "@embroider/macros": "^1.11.0",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-stew": "^3.0.0",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "velocity-animate": "^1.5.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^3.0.1",
    "@embroider/util": "^1.11.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.6.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.12.1",
    "ember-cli-fastboot": "^4.1.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-terser": "^4.0.2",
    "ember-code-snippet": "git+https://git@github.com/ef4/ember-code-snippet.git#d054b697098ad52481c94a952ccf8d89ba1f25fe",
    "ember-load-initializers": "^2.1.2",
    "ember-moment": "^10.0.0",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^8.0.0",
    "ember-sinon-qunit": "^7.1.4",
    "ember-source": "~4.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.10.1",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "jsdom": "^22.1.0",
    "loader.js": "^4.7.0",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "prember": "^2.0.0",
    "prettier": "^2.8.8",
    "prismjs": "^1.29.0",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "sass": "^1.63.3",
    "sinon": "^15.1.0",
    "webpack": "^5.86.0"
  },
  "peerDependencies": {
    "ember-source": ">= 4.0.0",
    "@ember/string": "^3.1.1"
  },
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/ember-animation/liquid-fire",
  "license": "MIT",
  "author": "Edward Faulkner <ef@alum.mit.edu>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "16.19.0",
    "yarn": "1.22.10"
  }
}